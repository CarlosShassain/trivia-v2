doctype html
html
  head
    title= title


    script(src="/javascripts/jquery.js")
    script(src="/javascripts/bootstrap.js")
    script(src="/javascripts/jquery.prettyPhoto.js")
    script(src="/javascripts/jquery.isotope.min.js")
    script(src="/javascripts/main.js")

    link(href="/stylesheets/bootstrap.min.css", rel="stylesheet")
    link(href="/stylesheets/font-awesome.min.css", rel="stylesheet")
    link(href="/stylesheets/prettyPhoto.css", rel="stylesheet")
    link(href="/stylesheets/animate.css", rel="stylesheet")
    link(href="/stylesheets/main.css", rel="stylesheet")
    script(src="/javascripts/site.js")
    script(src="/socket.io/socket.io.js")

    

    
  body
    header(class="navbar navbar-inverse navbar-fixed-top wet-asphalt" role="banner")
      div(class="container")
        div(class="navbar-header")
          button(type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse")
            span (class="sr-only") Toggle navigation
            span (class="icon-bar")
            span (class="icon-bar")
            span (class="icon-bar")
          h2 Triviador    
          h5 uatf
        div(class="collapse navbar-collapse")    
          ul(class="nav navbar-nav navbar-right")
            li(class="active")
               #{username}
            li(class="active")
              a(href="http://localhost:8000/") Home
            li(class="dropdown")
              a(href="#", class="dropdown-toggle", data-toggle="dropdown") otras paginas 
                i
                ul(class="dropdown-menu")
                  li
                    a(href="http://localhost:8000/editar/") editar datos 
                  li
                      a(href="#") seguridad
                  li(class="divider")
                    li
                    a(href="http://localhost:8000/logout/") privacidad
    div(id="ejemplo", class="modal fade")
     div(class="modal-dialog")   
      div(class="modal-content")
       div(class="modal-content") 
        div(class="modal-header ")
         button(type="button",class="close", data-dismiss="modal", aria-hidden="true") &times
         h4 este es el titulo
        div(class="modal-body bg-info")
         section(id="crearSala", class="container")
          div(class="row")
           div(class="col-sm-6")
            form(class="center",role="form", action="/espera/")
             fieldset(class="resgistration-form")
              div(class="form-group") Titulo
               input(type="text", id="titulo", name="titulo", placeholder="Titulo" class="form-control")
              div(class="form-group")
                div(class="radio")
                 label() privado
                  input(type="radio" name="tipo", value="privado")
               div(class="col-ms-6")
                div(class="radio")
                 label() publico
                  input(type="radio" name="tipo" value="publico", checked)
              div(class="form-group") Nro de Preguntas
               select(class="form-control", placeholder="Preguntas", name="pregunta")
                option(value="5") 5
                option(value="10") 10
                option(value="15") 15
                option(value="20") 20
              div(class="form-group") Tiempo de Respuesta
               select(class="form-control", placeholder="Preguntas", name="tiempo")
                option(value="10") 10 segundos
                option(value="15") 15 segundos
                option(value="20") 20 segundos
                option(value="25") 25 segundos
                option(value="30") 30 segundos
                option(value="35") 35 segundos
                option(value="40") 40 segundos
                option(value="45") 45 segundos
                option(value="50") 50 segundos
                option(value="55") 55 segundos
                option(value="60") 60 segundos
              div(class="form-group") 
               label() Radios
               div(class="col-ms-10")
                div(class="radio")
                 label() cat1
                  input(type="radio" name="categoria1")
               div(class="col-ms-10")
                div(class="radio")
                 label() cat2
                  input(type="radio" name="categoria2")
               div(class="col-ms-10")
                div(class="radio")
                 label() cat3
                  input(type="radio" name="categoria3")
              div(class="form-group")
               input(id="crearPartida", type="submit", class="btn btn-primary",value="crear")
        div(class="modal-footer")
         button(type="button",class="close", data-dismiss="modal") cerrar        
    div(id="boto", class="modal fade")
     div(class="modal-dialog")   
      div(class="modal-content")
       div(class="modal-header ")
        button(type="button",class="close", data-dismiss="modal", aria-hidden="true") &times
        h4 Pregunta de dota
       div(class="modal-body bg-info")
         div(class="row")
          div(class="col-sm-6")
           form(class="center",role="form", action="/espera/")
            fieldset(class="resgistration-form")
             div(class="form-group") pregunta
              input(type="text text-large", id="titulo", name="titulo", placeholder="pregunta" class="form-control", value="Costo de un bracer")
             div(class="form-group")
              input(id="crearPartida", type="submit", class="btn btn-primary",value="El costo es 450 en oro")
             div(class="form-group")
              input(id="crearPartida", type="submit", class="btn btn-primary",value="El costo es 150 en oro")
             div(class="form-group")
              input(id="crearPartida", type="submit", class="btn btn-primary",value="El costo es 1450 en oro")
             div(class="form-group")
              input(id="crearPartida", type="submit", class="btn btn-primary",value="El costo es 200 en oro")
             div(class="form-group")
              div(class="progress")
               div(class="progress-bar progress-bar-danger", role="progressbar", aria-valuenow="90", aria-valuemin="0", aria-valuemax="100", style="width: 90%;")
                span tiempo
       div(class="modal-footer")
        button(type="button",class="close", data-dismiss="modal") cerrar  
    block content
    
    